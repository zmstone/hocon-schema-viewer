actions.http.my_action.parameters {
  path = "/room/${room_no}"
  method = post
  headers = {
    accept = "application/json"
    cache-control = "no-cache"
    connection = "keep-alive"
    content-type = "application/json"
    keep-alive = "timeout=5"
  }
  body = "/body_template"
  max_retries = 2
}